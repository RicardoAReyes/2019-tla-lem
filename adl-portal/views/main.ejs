<!doctype html>
<html lang="en">
<%- include partials/sidenavhelper %>
<%- include("partials/header.ejs") %>

<body>
    <%- include partials/topnav %>
    <!-- Side Nav -->
    <div id="mySidebar" class="sidebar light-blue lighten-5 h-100" style="width: 18rem">
        <% include partials/sidenav %>
    </div>

    <!-- Nav tabs -->
    <div class="d-flex justify-content-start h-100" id="main" style="margin-top: 7rem; margin-left: 18rem">
        <!-- Side Nav tabs -->
        <!-- Center workstation. -->
        <div id="workstation" class="col-lg-12 col-md-10 align-items-center">
            <div class="border border-0 p-0 h-100">
                <% if (type == "experience") { %>
                <% include partials/experienceCards %>
                <% } else if (type == "landing") { %>
                <% include partials/landing %>
                <% } else { %>
                <% include partials/cassframe %>
                <% } %>
            </div>
        </div>
    </div>

    <!-- Button trigger modal -->
    <div class="modal" id="detailsModal" tabindex="-1" role="dialog">
        <div class="modal-dialog modal-side modal-bottom-left" role="document">
            <div class="modal-content">
                <div class="modal-header light-blue darken-3">
                    <h4 class="modal-title w-100 text-white" id="myModalLabel">Details</h4>
                    <button type="button" class="close m-0" data-dismiss="modal" aria-label="Close">
                        <span class="text-white" aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body" id="detailsModalBody"></div>
                <div class="modal-footer"></div>
            </div>
        </div>
    </div>

    <div id="snackbar"></div>

    <!-- Tab panels -->
    <%- include partials/element-builder %>
    <%- include partials/utilities %>
    <%- include partials/footer %>

</body>

</html>